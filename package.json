	"scripts": {
		"start": "npm run dev",
		"dev": "npm run clear && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js --watch --progress --colors",
		"test": "cross-env  NODE_ENV=production  webpack --config build/webpack.test.js && npm run clearMap",
		"build": "cross-env NODE_ENV=production webpack --config build/webpack.pro.js && npm run clearMap",
		"rc": "cross-env NODE_ENV=production webpack --config build/webpack.rc.js && npm run clearMap",
		"clear": "rimraf ./dist",
		"push-rc": "npm run rc && cp -rf ./upload-rc.sh ./dist && cd dist && sh upload-rc.sh",
		"push-test": "rm -rf node_modules && yarn && npm run test && cp -rf ./upload.sh ./dist && cd dist && npm run clearApk && sh upload.sh",
		"clearMap": "rimraf ./dist/*.map",
		"clearApk": "rimraf ./dist/*.apk",
		"precommit": "pretty-quick --staged",
		"prettify": "prettier --fix --write 'src/pages/login/login_page/index.js'"
	},
	"lint-staged": {
		"scripts": {
			"precommit": "lint-staged",
			"lint-staged": {
